{
  "name": "easy-annotation",
  "displayName": "Easy Annotation",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "publisher": "hoon",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/juimwonsa/easy-annotation.git"
  },
  "bugs": {
    "url": "https://github.com/juimwonsa/easy-annotation/issues"
  },
  "homepage": "https://github.com/juimwonsa/easy-annotation#readme",
  "keywords": [
    "html",
    "javascript",
    "css",
    "comment",
    "toggle comment",
    "textmate"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "easy-annotation.toggleComment",
        "title": "Toggle Smart Comment (HTML/JS)"
      }
    ],
    "keybindings": [
      {
        "command": "easy-annotation.toggleComment",
        "key": "ctrl+/",
        "mac": "cmd+/",
        "when": "editorTextFocus && editorLangId == 'html'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "vscode-oniguruma": "^2.0.1",
    "vscode-textmate": "^9.2.0"
  }
}
